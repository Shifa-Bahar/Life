<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools">

  <data>

    <import type="android.view.View" />

    <variable
      name="click"
      type="com.lifepharmacy.application.ui.address.ClickSelectedAddress" />

    <variable
      name="mViewModel"
      type="com.lifepharmacy.application.ui.address.AddressViewModel" />

    <variable
      name="selectedAddress"
      type="androidx.databinding.ObservableField&lt;com.lifepharmacy.application.model.address.AddressModel&gt;" />
  </data>


<!--  android:background="#214f9f"-->
  <androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_40dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:background="#214f9f"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/tv_select"
      style="@style/text_14_1d1d_open_regular"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="56dp"
      android:layout_marginTop="3dp"
      android:layout_marginBottom="4dp"
      android:padding="@dimen/dimen_2dp"
      android:text="Select Test"
      android:textColor="@color/white"
      android:textSize="@dimen/dimen_10dp"
      app:layout_constraintBottom_toTopOf="@+id/img_product"
      app:layout_constraintEnd_toStartOf="@+id/tv_selects"
      app:layout_constraintHorizontal_bias="0.074"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.AppCompatImageView
      android:id="@+id/img_product"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="80dp"
      android:layout_marginBottom="4dp"
      android:src="@drawable/ic_yellowinwhite"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/tv_selects"
      style="@style/text_14_1d1d_open_regular"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="3dp"
      android:layout_marginEnd="44dp"
      android:layout_marginBottom="4dp"
      android:padding="@dimen/dimen_2dp"
      android:text="Address &amp; Slot"
      android:textColor="@color/white"
      android:textSize="@dimen/dimen_10dp"
      app:layout_constraintBottom_toTopOf="@+id/img_product"
      app:layout_constraintEnd_toStartOf="@+id/tv_selectss"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.AppCompatImageView
      android:id="@+id/img_products"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="1dp"
      android:layout_marginBottom="4dp"
      android:src="@drawable/ic_grayinwhite"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@+id/view10s"
      app:layout_constraintTop_toBottomOf="@+id/tv_selects"
      tools:ignore="MissingConstraints" />

    <View
      android:id="@+id/view10s"
      android:layout_width="100dp"
      android:layout_height="1dp"
      android:layout_gravity="center"
      android:background="@color/color_e8e8"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@+id/img_product"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.644"
      tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatTextView
      android:id="@+id/tv_selectss"
      style="@style/text_14_1d1d_open_regular"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="3dp"
      android:layout_marginEnd="68dp"
      android:layout_marginBottom="4dp"
      android:padding="@dimen/dimen_2dp"
      android:text="Payment"
      android:textColor="@color/white"
      android:textSize="@dimen/dimen_10dp"
      app:layout_constraintBottom_toTopOf="@+id/img_product"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

    <View
      android:id="@+id/view10ss"
      android:layout_width="88dp"
      android:layout_height="1dp"
      android:layout_gravity="center"
      android:layout_marginEnd="2dp"
      android:background="@color/color_e8e8"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/img_productss"
      app:layout_constraintStart_toEndOf="@+id/img_products"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.666"
      tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatImageView
      android:id="@+id/img_productss"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="92dp"
      android:layout_marginBottom="4dp"
      android:src="@drawable/ic_grayinwhite"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      tools:ignore="MissingConstraints" />


  </androidx.constraintlayout.widget.ConstraintLayout>
</layout>